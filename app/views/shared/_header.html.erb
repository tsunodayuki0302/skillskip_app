<header class='top-page-header'>
  <div class='nav'>
    <ul class='lists-left'>
      <%#= link_to image_tag(".png", class:"user-icon"), "/" %>
      <li><%#= link_to current_user.nickname, '#', class: "user-nickname" %></li>
    <ul class='lists-right'>
    <% if user_signed_in? %>
    <li><%#= link_to image_tag(".png", class: "search" ), "/" %></li>
    <li><%#= link_to image_tag(".png", class: "community" ), "/" %></li>
    <li><%#= link_to image_tag(".png", class: "room" ), "/" %></li>
    <% else %>
      <li><%= link_to 'ログイン', new_user_session_path, class: "login" %></li>
      <li><%= link_to '新規登録', new_user_registration_path, class: "sign-up" %></li>
    <% end %>
    </ul>
  </div>
</header>